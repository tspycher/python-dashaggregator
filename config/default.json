{
	"version": 1,
	"allow_edit": true,
	"plugins": [],
	"panes": [
		{
			"title": "Local Weather",
			"width": 1,
			"row": {
				"3": 1,
				"4": 1
			},
			"col": {
				"3": 1,
				"4": 4
			},
			"col_width": "1",
			"widgets": [
				{
					"type": "text_widget",
					"settings": {
						"title": "Temperature",
						"sparkline": false,
						"value": "datasources[\"NetatmoLSZI\"][\"data\"][\"Temperature\"]",
						"units": "°",
						"animate": true,
						"size": "regular"
					}
				},
				{
					"type": "text_widget",
					"settings": {
						"units": "hpa",
						"animate": true,
						"title": "Pressure",
						"value": "datasources[\"SchupfartADInfo\"][\"data\"][\"hpa\"]",
						"size": "regular"
					}
				},
				{
					"type": "text_widget",
					"settings": {
						"units": "ft",
						"animate": true,
						"title": "DA",
						"value": "datasources[\"SchupfartADInfo\"][\"data\"][\"da\"]",
						"size": "regular"
					}
				}
			]
		},
		{
			"title": "LSZI Wind",
			"width": 1,
			"row": {
				"3": 1,
				"4": 1
			},
			"col": {
				"3": 2,
				"4": 2
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "winddir_plugin",
					"settings": {
						"direction": "datasources[\"BaselADInfo\"][\"data\"][\"winddir\"]",
						"speed": "datasources[\"BaselADInfo\"][\"data\"][\"wind\"]"
					}
				},
				{
					"type": "gauge",
					"settings": {
						"units": "kt",
						"max_value": "40",
						"min_value": 0,
						"value": "datasources[\"SchupfartADInfo\"][\"data\"][\"wind\"]",
						"title": "Wind"
					}
				},
				{
					"type": "sparkline",
					"settings": {
						"value": [
							"datasources[\"SchupfartADInfo\"][\"data\"][\"wind\"]"
						],
						"legend": "kt",
						"include_legend": false,
						"title": "Wind History"
					}
				},
				{
					"type": "text_widget",
					"settings": {
						"units": "°",
						"animate": true,
						"sparkline": true,
						"value": "datasources[\"SchupfartADInfo\"][\"data\"][\"winddir\"]",
						"size": "regular"
					}
				}
			]
		},
